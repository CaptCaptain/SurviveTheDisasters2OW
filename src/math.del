class Math {
    public static Number pow(Number base, Number exponent): base ^ exponent;
}

class EasingFunctions {
    public static Number easeInExpo(Number x): x == 0 ? 0 : Math.pow(2, 10 * x - 10);

    public static Number easeInQuad(Number x): x * x;

    public static Number easeOutExpo(Number x): x == 1 ? 1 : 1 - Math.pow(2, -10 * x);
}